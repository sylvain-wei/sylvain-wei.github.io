  shorttitle = {Towards {{Revealing}} the {{Mystery}} behind {{Chain}} of {{Thought}}},
  author = {Feng, Guhao and Zhang, Bohang and Gu, Yuntian and Ye, Haotian and He, Di and Wang, Liwei},
